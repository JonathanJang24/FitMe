<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fitme.client.ClientController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Home">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="greetingText" layoutX="24.0" layoutY="27.0" prefHeight="47.0" prefWidth="547.0" text="Hello ">
                           <font>
                              <Font size="38.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Food">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox layoutX="14.0" layoutY="15.0" prefHeight="321.0" prefWidth="161.0" AnchorPane.leftAnchor="14.0">
                           <children>
                              <Label prefHeight="17.0" prefWidth="141.0" text="Food Macro Entry">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                              <Label prefHeight="17.0" prefWidth="141.0" text="Name" />
                              <TextField fx:id="macroEntryNameField" />
                              <Label prefWidth="141.0" text="Calories" />
                              <TextField fx:id="macroEntryCaloriesField" />
                              <Label prefWidth="141.0" text="Protein" />
                              <TextField fx:id="macroEntryProteinField" />
                              <Label prefWidth="141.0" text="Fibers" />
                              <TextField fx:id="macroEntryFiberField" />
                              <Label fx:id="macroEntryCarbohydrates" prefWidth="141.0" text="Carbohydrates" />
                              <TextField fx:id="macroEntryCarbohydratesField" />
                              <Label prefWidth="141.0" text="Fats" />
                              <TextField fx:id="macroEntryFatsField" />
                              <Button fx:id="addFoodMacroButton" mnemonicParsing="false" text="Add Food">
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                        <VBox layoutX="190.0" layoutY="15.0" prefHeight="89.0" prefWidth="178.0">
                           <children>
                              <Label text="Record Food">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                              <Label text="Food" />
                              <Label text="Idk wut menu selector for existing fooods" />
                              <Label text="Date consumed" />
                              <DatePicker />
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
            <Tab text="Workout">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab text="About Me">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
