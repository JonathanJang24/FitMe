<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fitme.signup.SignupController">
   <children>
      <Label layoutX="106.0" layoutY="38.0" text="Sign Up for FitMe">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="signupUsernameField" layoutX="169.0" layoutY="103.0" />
      <TextField fx:id="signupPasswordField" layoutX="169.0" layoutY="135.0" />
      <Label layoutX="49.0" layoutY="107.0" text="Username" />
      <Label layoutX="48.0" layoutY="139.0" text="Password" />
      <Button fx:id="createAccountButton" layoutX="149.0" layoutY="345.0" mnemonicParsing="false" onAction="#createAccount" text="Create Account" />
   </children>
</AnchorPane>
